{
"manifest_version": 3,
"name": "Phishing Detector",
"version": "1.0",
"description": "Check if the current page is phishing using a local FastAPI model.",
"permissions": ["tabs"],  
"host_permissions": [
  "<all_urls>",           
  "http://127.0.0.1:8000/*" 
],
"action": {
  "default_popup": "popup.html",
  "default_title": "Check for phishing"
},
"icons": {
  "16": "icon16.png",
  "48": "icon48.png",
  "128": "icon128.png"
}
}

